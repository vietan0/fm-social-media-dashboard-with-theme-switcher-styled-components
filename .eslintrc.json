{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "plugin:react/recommended", "plugin:react/jsx-runtime", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "jsx-a11y/label-has-associated-control": "off"
    }
}
